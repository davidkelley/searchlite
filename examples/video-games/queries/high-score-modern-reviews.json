{
  "query": "performance polished accessibility",
  "fields": null,
  "limit": 5,
  "execution": "wand",
  "bmw_block_size": null,
  "return_stored": true,
  "highlight_field": "text",
  "filter": {
    "And": [
      {
        "KeywordEq": {
          "field": "doc_type",
          "value": "review"
        }
      },
      {
        "KeywordEq": {
          "field": "era",
          "value": "modern"
        }
      },
      {
        "F64Range": {
          "field": "review_score_out_of_10",
          "min": 8.5,
          "max": 10.0
        }
      },
      {
        "I64Range": {
          "field": "release_year",
          "min": 2015,
          "max": 2025
        }
      },
      {
        "KeywordIn": {
          "field": "platforms",
          "values": [
            "PC",
            "PlayStation 5",
            "Xbox Series X|S"
          ]
        }
      }
    ]
  }
}
