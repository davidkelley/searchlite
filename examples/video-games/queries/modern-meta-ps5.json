{
  "query": "meta tier loadout",
  "fields": null,
  "limit": 5,
  "execution": "wand",
  "bmw_block_size": null,
  "return_stored": true,
  "highlight_field": "text",
  "filter": {
    "And": [
      {
        "KeywordEq": {
          "field": "doc_type",
          "value": "competitive_meta_report"
        }
      },
      {
        "KeywordEq": {
          "field": "era",
          "value": "modern"
        }
      },
      {
        "KeywordEq": {
          "field": "platforms",
          "value": "PlayStation 5"
        }
      },
      {
        "KeywordIn": {
          "field": "genres",
          "values": [
            "fighting",
            "FPS",
            "JRPG"
          ]
        }
      }
    ]
  }
}
